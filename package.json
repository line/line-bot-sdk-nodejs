{
  "name": "@line/bot-sdk",
  "version": "6.8.4",
  "description": "Node.js SDK for LINE Messaging API",
  "engines": {
    "node": ">=8"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "pretest": "npm run format && npm run build",
    "test": "TEST_PORT=1234 TS_NODE_CACHE=0 nyc mocha",
    "prettier": "prettier --parser typescript --trailing-comma all \"{lib,test}/**/*.ts\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- -l",
    "clean": "del-cli dist",
    "prebuild": "npm run format:check && npm run clean",
    "build": "tsc",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./deploy-docs.sh",
    "release": "npm run build && npm publish --access public",
    "precommit": "npm run format:check",
    "prepush": "npm run format:check && npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:line/line-bot-sdk-nodejs.git"
  },
  "keywords": [
    "node",
    "line",
    "sdk"
  ],
  "dependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/file-type": "^5.2.2",
    "@types/node": "^7.10.9",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "file-type": "^7.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/finalhandler": "^1.1.0",
    "@types/mocha": "^2.2.48",
    "del-cli": "^1.1.0",
    "express": "^4.17.1",
    "finalhandler": "^1.1.2",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "nock": "^11.7.0",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "vuepress": "^0.14.11"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "license": "Apache-2.0"
}
